html,body{font-family:public sans,sans-serif;background-color:#fff8ee}img{padding:5px;border:1px solid #ccc}.is-borderless{border:0}.pale-yellow-bg{background-color:#fff8ee}.sawarabi-mincho-regular{font-family:sawarabi mincho,serif;font-weight:400;font-style:normal}.button-primary{-webkit-border-radius:1px;-moz-border-radius:1px;border-radius:1px;color:#fff;font-size:16px;background:#000;margin-bottom:.5rem;border:solid #000 1px;text-decoration:none;transition:all .2s ease-out;justify-content:center;padding-bottom:calc(.5em - 1px);padding-left:1em;padding-right:1em;padding-top:calc(.5em - 1px);text-align:center;white-space:nowrap}.button-primary:hover{background:#fff;text-decoration:none;color:#000;border:solid #000 1px;box-shadow:5px 5px #000000}a{text-decoration:none;transition:all .15s ease-out}a:hover{background-color:transparent;text-decoration:underline;font-weight:700}a.navbar-item:hover{background-color:transparent;color:inherit;transform:scale(1.1)}